// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource mysql {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "mysql"
}

generator client {
  provider = "prisma-client-js"
}

model Payment {
  id           String    @id @default(cuid())
  store        Store?    @relation("Payment_store", fields: [storeId], references: [id])
  storeId      String?   @map("store")
  amount       Int?
  payment_date DateTime?
  created_at   DateTime? @default(now())

  @@index([storeId])
}

model Mall {
  id         String    @id @default(cuid())
  name       String    @default("")
  location   String    @default("")
  created_at DateTime? @default(now())
  stores     Store[]   @relation("Mall_stores")
}

model Store {
  id         String    @id @default(cuid())
  name       String    @default("")
  malls      Mall[]    @relation("Mall_stores")
  client     Client?   @relation("Store_client", fields: [clientId], references: [id])
  clientId   String?   @map("client")
  created_at DateTime? @default(now())
  payments   Payment[] @relation("Payment_store")

  @@index([clientId])
}

model Client {
  id                String    @id @default(cuid())
  name              String    @default("")
  contact           String    @default("")
  email             String    @default("")
  created_at        DateTime? @default(now())
  from_Store_client Store[]   @relation("Store_client")
}

model User {
  id       String  @id @default(cuid())
  username String  @default("")
  password String?
  email    String  @unique @default("")
  role     String  @default("user")
}
